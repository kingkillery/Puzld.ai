{
  "name": "Read-Only Code Questions",
  "description": "Tasks that only require reading and analyzing code - no modifications allowed",
  "tasks": [
    {
      "id": "read-01-find-functions",
      "category": "read",
      "description": "Find all exported functions in a TypeScript file",
      "prompt": "List all exported functions in src/agentic/tools/index.ts. Just list their names.",
      "expectedBehavior": {
        "toolsAllowed": ["view", "glob", "grep"],
        "toolsDenied": ["write", "edit", "bash"],
        "maxIterations": 10,
        "mustNotWrite": true
      },
      "verification": {
        "outputContains": ["executeTool", "executeTools", "getTool"],
        "filesNotModified": ["src/agentic/tools/index.ts"]
      },
      "tags": ["read", "typescript", "functions"]
    },
